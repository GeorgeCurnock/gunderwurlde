<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" style="-fx-spacing: 15px;" stylesheets="@../css/menu_stylesheet.css" type="VBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Help">
         <font>
            <Font name="Manaspace" size="50.0" />
         </font>
         <styleClass>
            <String fx:value="menuTitle" />
            <String fx:value="manaspaceFont" />
         </styleClass>
      </Label>
      <Label styleClass="manaspaceFont" text="[Insert instructions]">
         <font>
            <Font name="Manaspace" size="25.0" />
         </font>
      </Label>
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonPress" text="Back">
         <font>
            <Font name="Manaspace" size="22.0" />
         </font>
         <styleClass>
            <String fx:value="menuButton" />
            <String fx:value="manaspaceFont" />
         </styleClass>
      </Button>
   </children>
</fx:root>
